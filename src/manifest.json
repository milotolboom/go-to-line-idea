{
  "name": "IDEA github opener",
  "version": "1.0",
  "description": "Extension that let's you easily open files while reviewing pull requests",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/turtle_16.png",
      "32": "img/turtle_32.png",
      "48": "img/turtle_64.png",
      "128": "img/turtle_128.png"
    }
  },
  "icons": {
    "16": "img/turtle_16.png",
    "32": "img/turtle_32.png",
    "48": "img/turtle_64.png",
    "128": "img/turtle_128.png"
  },
  "manifest_version": 2
}
